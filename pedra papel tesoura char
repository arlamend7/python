import random
 
ids = ['pedra', 'papel', 'tissoura']
 
escolha_computador = random.choice(ids) 

num = str(input('pedra, papel ou tesoura? \n')) 

if num == random.choice(ids):
    print ('empate')
elif (num == 'pedra' and escolha_computador == 'papel') or (num == 'tesoura' and escolha_computador == 'pedra') or (num == 'papel' and escolha_computador == 'tesoura'):
    print ('computador ganhou')
elif (num == 'papel' and escolha_computador == 'pedra') or num == 'pedra' and escolha_computador == 'tesoura') or (num == 'tesoura' and escolha_computador == 'papel'):
    print ('voce ganhou')
else :
    print ('blz?')
